{
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "baseBranches": [],
    "enableLicenseViolations": true,
    "enableReachability": true, 
    "exploitability": true
  },
  "imageSettings": {
    "imageTracing": {
        "enableImageTracingPR": true,
        "addDockerfilePath": true,
        "addRepositoryCoordinates": true
    }
  },
   "scanSettingsSAST": {
    "enableScan": true,
    "enableRemediation": true,
    "scanPullRequests": true,
    "incrementalScan": true,
    "baseBranches": [],
    "snippetSize": 10
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff",
    "useMendCheckNames": true,
    "strictMode": "warning"
  },
  "checkRunSettingsSAST": {
    "checkRunConclusionLevel": "failure",
    "severityThreshold": "high"
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
 "remediateSettings": {
    "enableRenovate": true,
        "extends": ["mergeConfidence:all-badges"]
  }
   "packageRules": [
     {
      	 "groupName": "Mend: high confidence minor and patch dependency updates",
      	 "matchUpdateTypes": ["minor", "patch"],
      	 "matchConfidence": ["very high", "high"]
     },  

     
     


























 

